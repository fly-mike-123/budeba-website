generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model QuoteRequest {
  id           String   @id @default(cuid())
  fullName     String
  phone        String
  email        String?
  company      String?
  location     String?
  service      String
  items        String
  budget       String?
  deliveryDate String?
  status       String   @default("new")
  createdAt    DateTime @default(now())
}
